#include <iostream>
#include <vector>
using namespace std;


int main()
{
	int N;
	cin >> N;
	vector<int> arr_1, arr_2;
	for (int i = 0; i < N; i++)
	{
		int tmp, tmp2;
		cin >> tmp >> tmp2;
		arr_1.push_back(tmp);
		arr_2.push_back(tmp2);
	}

	for (int i = 0; i < N; i++)
	{
		cout << "Case #" << i + 1 << ": " << arr_1[i] << " + " << arr_2[i] << " = " << arr_1[i] + arr_2[i] << '\n';
	}
}
