#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int Kook, Sae=0;
    int j,s,k,y;
    cin>>j>>s>>k>>y;
    Kook=j+k+s+y;
    cin>>j>>s>>k>>y;
    Sae=j+k+s+y;
    
    if(Kook>=Sae)cout<<Kook<<'\n';
    else cout<<Sae<<'\n';
    return 0;
}

